# .bash_aliases
#
# Bash aliases loaded by the Codespaces `.bashrc` file.

# System aliases.
alias ll='ls -lah'

# Git aliases.
alias g='git'
alias ga='git a'
alias gax='git ax'
alias gd='git diff'
alias gl='git lg'
alias gs='git status'
alias gcm='git commit'

# Laravel aliases.
alias sail='./vendor/bin/sail'
